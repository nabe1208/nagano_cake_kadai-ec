<h1>配送先編集</h1>
<%= form_with model: @address, url: public_address_path(@address.id), method: :patch do |f| %>
<p>
  <%= f.label "郵便番号(ハイフンなし)" %>
  <%= f.number_field :postal_code %>
</p>
<p>
  <%= f.label "住所" %>
  <%= f.text_field :address %>
</p>
<p>
  <%= f.label "宛名" %>
  <%= f.text_field :name%>
  <%= f.submit "変更内容を保存"%>
</p>
<% end %>
